generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Receipts {
  id        String   @id @default(cuid())
  receipt   String   @db.LongText
  title     String?
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  type      String?
}
